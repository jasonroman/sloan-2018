{% set canonicalUrl = url(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) %}

{% set route = path(app.request.attributes.get('_route'), app.request.attributes.get('_route_params')) %}
{% set routeName = app.request.attributes.get('_route') %}
{% set queryString = app.request.queryString ? ('?' ~ app.request.queryString) : '' %}
{% set queryStringAppend = app.request.query.all|length ? '&' : '?' %}
{% set fullRoute = route ~ queryString %}

<!DOCTYPE html>
<html>
    <head>
        {% block googleAnalytics %}{% endblock %}
        {% block meta %}<meta charset="UTF-8">{% endblock %}
        <title>{% block title %}Sloan 2018 - REST API and Json Graphing - Jason Roman{% endblock %}</title>
        {% block stylesheets %}{% endblock %}
        {% block headerJavascripts %}{% endblock %}
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}{% endblock %}
    </body>
</html>
